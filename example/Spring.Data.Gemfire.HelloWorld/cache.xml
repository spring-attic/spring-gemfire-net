<?xml version="1.0"?>

<!-- cache.xml
     Configures a server to for clients at port 40404.
-->

<!DOCTYPE cache PUBLIC "-//GemStone Systems, Inc.//GemFire Declarative Cache 6.0//EN" "https://www.gemstone.com/dtd/cache6_0.dtd">
<cache>
  <cache-server port="40404"/>
  
  <region name="exampleregion">
    <region-attributes data-policy="replicate" >
    </region-attributes>
    <region name="exampleregion">
      <region-attributes data-policy="replicate">
      </region-attributes>
    </region>
  </region>
 
</cache>
